{
	"family": "fs-td",
	"executionRoleArn": "arn:aws:iam::755064992656:role/ecsTaskExecutionRole",
	"networkMode": "awsvpc",
	"containerDefinitions": [
		{
			"name": "container-1",
			"image": "<IMAGE1_NAME>",
			"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "/ecs/fs-td",
					"awslogs-region": "us-east-1",
					"awslogs-stream-prefix": "ecs",
					"awslogs-multiline-pattern": "^LOG"
				}
			},

			"portMappings": [
				{
					"hostPort": 5000,
					"protocol": "tcp",
					"containerPort": 5000
				}
			],
			"essential": true
		}
	],

	"cpu": "256",
	"memory": "512",
	"requiresCompatibilities": ["FARGATE"]
}
