{
	"family": "fs-td",
	"executionRoleArn": "arn:aws:iam::755064992656:role/ecsTaskExecutionRole",
	"networkMode": "awsvpc",
	"containerDefinitions": [
		{
			"name": "fs-write-container",
			"image": "<IMAGE1_NAME>",
			"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "/ecs/fs-td",
					"awslogs-region": "us-east-1",
					"awslogs-stream-prefix": "ecs"
				}
			},

			"portMappings": [
				{
					"hostPort": 5000,
					"protocol": "tcp",
					"containerPort": 5000
				}
			],
			"essential": true
		},

		{
			"name": "fs-read-container",
			"image": "<IMAGE2_NAME>",

			"portMappings": [
				{
					"hostPort": 5001,
					"protocol": "tcp",
					"containerPort": 5001
				}
			],
			"essential": true
		}
	],
	"cpu": "256",
	"memory": "512",
	"requiresCompatibilities": ["FARGATE"]
}
